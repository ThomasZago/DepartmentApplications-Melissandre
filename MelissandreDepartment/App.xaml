             xmlns:local="clr-namespace:MelissandreDepartment"
             xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
             StartupUri="View/MainTechnicalWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/MelissandreServiceLibrary;component/StylesDictionary.xaml" />
